<MTIfNonZero tag="AssetCount">
<dt>Photos</dt>
<dd class="recentAssets">
	<ul class="thumb">
	<MTAssets type="image">
		<li>
			<a href="<$MTAssetURL$>" rel="nofollow">
					<img src="<$MTAssetThumbnailURL height="70"$>" alt="<$MTAssetLabel$>" />
			</a>
		</li>
	</MTAssets>
	</ul>
</dd>
</MTIfNonZero>
